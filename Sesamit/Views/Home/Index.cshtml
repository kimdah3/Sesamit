@model dynamic

@{
    ViewBag.Title = "Sesamit";
    Layout = "../Shared/_Layout.cshtml";
}


<div class="header-slide">
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
            <li data-target="#carousel-example-generic" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <div class="carousel-caption">
                        <span class="glyphicon glyphicon-search"></span>
                        <p>Ska du plugga ekonomi, informatik eller statistik?</p>
                        <p>Grattis du är en Sesamit!</p>
                        <p>LÄS MER HÄR</p>
                </div>
            </div>
            <div class="item">
                <div class="carousel-caption">
                        <span class="glyphicon glyphicon-briefcase"></span>
                        <p>
                            Sesamit.se kommer med ständiga uppdateringar till jobbmöjligheter från olika jobbaktörer. Så håll utkik!
                        </p>
                        <p>Läs mer här</p> 
                </div>
            </div>
            <div class="item">
                <div class="carousel-caption">
                    <div class="well">
                        <span class="glyphicon glyphicon-briefcase"></span>
                        <p>För även andra erbjudanden, följ oss på Facebook!</p>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

