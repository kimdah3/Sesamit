@{
    ViewData["Title"] = "Framsida";
}

<header class="front-page-header">
    <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#carousel-example-generic" data-slide-to="0" class="active"></li>
            <li data-target="#carousel-example-generic" data-slide-to="1"></li>
        </ol>
        <!-- Wrapper for slides -->
        <div class="carousel-inner" role="listbox">
            <div class="item active">
                <div class="carousel-caption">
                    <i class="fa fa-exclamation fa-5x" aria-hidden="true"></i>
                    <h1>Höstens introduktion drar igång den 26 augusti</h1>
                    @*<h1>Ska du plugga ekonomi, informatik eller statistik?</h1>*@
                    @*<h1>Grattis du är en Sesamit!</h1>*@
                    <p>

                        <a asp-controller="Home" asp-action="Intro" class="btn btn-primary">Klicka här för att veta mer!</a>
                    </p>
                </div>
            </div>
            <div class="item">
                <div class="carousel-caption">
                    <i class="fa fa-briefcase" aria-hidden="true"></i>
                    <h1>Jobbmöjligheter!</h1>
                    <h4>
                        Håll utkik! Vi kommer med ständiga uppdateringar från olika jobbaktörer.
                    </h4>
                    <a asp-controller="NyheterBlogg" asp-action="Index" class="btn btn-info">Läs mer!</a>
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#carousel-example-generic" role="button" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#carousel-example-generic" role="button" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>

    </div>
</header>
<div class="alert alert-dismissible alert-info text-center">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <strong>Senaste nytt:</strong> Unionen söker studenter till extrajobb i Örebro!
</div>

<section class="section-padding" id="section-frontpage">

    <div class="container">
        <div class="row">
            <div class="col-md-9 col-sm-9 col-xs-9">
                <h1 class="">Vad är Sesam?</h1>
                <p>
                    <span class="lead">Vi är kårsektionen för dig som läser <strong>ekonomi</strong>, <strong>informatik</strong> och <strong>statistik</strong>. </span>
                    Vi grundades 1993 och har sedan dess funnits med syfte att bevaka utbildningen, engagera medlemmar socialt och ge dem en naturligare väg in i näringslivet efter studierna.
                    Återkommande aktiviteter är bland annat Mentorskapsprojektet, Campusmässan, Båtfesten och <a asp-controller="Home" asp-action="Intro">Introduktionen</a>. Vi är stolta över vår introduktion som är en av Sveriges längsta och bästa i slaget, där du som ny student snabbt har möjlighet att komma in i studentlivet!
                </p>


                <div class="row text-center row-top-buffer row-bottom-buffer">
                    <div class="col-md-3 col-xs-6 fa-blue">
                        <i class="fa fa-users fa-3x" aria-hidden="true"></i>
                        <h4>
                            Över 500
                            unika Medlemmar!
                        </h4>
                    </div>
                    <div class="col-md-3 col-xs-6">
                        <i class="fa fa-check fa-3x fa-blue" aria-hidden="true"></i>
                        <h4>
                            En sjukt bra
                            Introduktion!
                        </h4>
                    </div>
                    <div class="col-md-3 col-xs-6">
                        <i class="fa fa-suitcase fa-3x fa-blue" aria-hidden="true"></i>
                        <h4>
                            Samarbeten
                            med företag!
                        </h4>
                    </div>
                    <div class="col-md-3 col-xs-6">
                        <i class="fa fa-coffee fa-3x fa-blue" aria-hidden="true"></i>
                        <h4>
                            Vår egen
                            lounge i nova!
                        </h4>
                    </div>
                </div>
                @*<p>
                        <a asp-controller="OmSesam" asp-action="VadArSesam" class="btn btn-primary btn-lg t">Läs mer här!</a>
                    </p>*@
            </div>
            <div class="col-md-3 col-sm-3 col-xs-3">
                <img class="img-responsive img-logo-fp" src="../Images/sesamLogo-medium.png" />
            </div>
        </div>
    </div>
</section>


