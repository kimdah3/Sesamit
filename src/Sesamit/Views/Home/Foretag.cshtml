@{
    ViewData["Title"] = "Företag";
}
<header class="top-page-header" id="foretag-header">
    <div class="container text-center">
        <i class="fa fa-thumbs fa-5x" aria-hidden="true"></i>
        <h1>Våra samarbetspartners</h1>
        @*<h4>Varmt välkommen till Sesam ny student!</h4>*@
        <h4></h4>
    </div>
</header>

